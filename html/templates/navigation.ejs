<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Navigation</title>
        <% include head %>
    </head>
    <body>
        
        <div style="padding: 10px">
        
		<ol id='navigation-tree'>
            <li class='expanded'>
                <!-- ROOT -->
                <a href="<%=: project | toUrl %>" target="content">
                    <span class="node-icon <%=: project | toIcon %>"></span>
                    <%=: project | toName %>
                </a>
                <% if (project.getChildNodes().length > 0) { %>
                <ol>
                    <% project.getChildNodes().forEach(function (e1) { %>
                    <li>
                        
                        <!-- First Level -->
                        <a href="<%=: e1 | toUrl %>" target="content">
                            <span class="node-icon <%=: e1 | toIcon %>"></span>
                            <%=: e1 | toName %>
                        </a>
                        <% if (e1.getChildNodes().length > 0) { %>
                        <ol>
                            <% e1.getChildNodes().forEach(function (e2) { %>
                            <li>
                                
                                <!-- Second Level -->                                
                                <a href="<%=: e2 | toUrl %>" target="content"><%=: e2 | toName %></a>
                                <% if (e2.getChildNodes().length > 0) { %>
                                <ol>
                                    <% e2.getChildNodes().forEach(function (e3) { %>
                                    <li>

                                        <!-- Third Level -->
                                        <a href="<%=: e3 | toUrl %>" target="content"><%=: e3 | toName %></a>
                                        <% if (e3.getChildNodes().length > 0) { %>
                                        <ol>
                                            <% e3.getChildNodes().forEach(function (e4) { %>
                                            <li>
                                                
                                                <!-- Fourth Level -->
                                                <a href="<%=: e4 | toUrl %>" target="content"><%=: e4 | toName %></a>
                                                <% if (e4.getChildNodes().length > 0) { %>
                                                <ol>
                                                    <% e4.getChildNodes().forEach(function (e5) { %>
                                                    <li>
                                                        
                                                        <!-- Fifth Level -->
                                                        <a href="<%=: e5 | toUrl %>" target="content"><%=: e5 | toName %></a>
                                                        <% if (e5.getChildNodes().length > 0) { %>
                                                        <ol>
                                                            <% e5.getChildNodes().forEach(function (e6) { %>
                                                            <li>
                                                                
                                                                <!-- Sixth Level -->
                                                                <a href="<%=: e6 | toUrl %>" target="content"><%=: e6 | toName %></a>
                                                                <% if (e6.getChildNodes().length > 0) { %>
                                                                <ol>
                                                                    <% e6.getChildNodes().forEach(function (e7) { %>
                                                                    <li>

                                                                        <!-- Seventh Level -->
                                                                        <a href="<%=: e7 | toUrl %>" target="content"><%=: e7 | toName %></a>

                                                                    </li>
                                                                    <% }); %>
                                                                </ol>
                                                                <% } %>
                                                                
                                                            </li>
                                                            <% }); %>
                                                        </ol>
                                                        <% } %>
                                                        
                                                    </li>
                                                    <% }); %>
                                                </ol>
                                                <% } %>
                                                
                                            </li>
                                            <% }); %>
                                        </ol>
                                        <% } %>
                                        
                                    </li>
                                    <% }); %>
                                </ol>
                                <% } %>
                                
                            </li>
                            <% }); %>
                        </ol>
                        <% } %>
                        
                    </li>
                    <% }); %>
                </ol>
                <% } %>
                
            </li>
		</ol>

        </div>            
            
		<script>
			$('#navigation-tree').bonsai();
		</script>        
        
    </body>    
</html>
